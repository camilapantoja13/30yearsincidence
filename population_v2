assign_quintile_2010 <- function(score) {
  case_when(
    score >= 34.21           ~ 1,  # Most deprived
    score >= 21.15           ~ 2,
    score >= 12.72           ~ 3,
    score >= 8.35            ~ 4,
    !is.na(score)            ~ 5,  # Least deprived
    TRUE                     ~ NA_integer_
  )
}

df_2011_adults <- df_2011_adults %>%
  mutate(imd_quintile = assign_quintile_2010(imd_score))

