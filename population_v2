20%= 29.52
40% = 23.9
60%= 17.8
80% = 12.85

assign_quintile_2021 <- function(score) {
  case_when(
    score >= 29.52           ~ 1,  # Most deprived
    score >= 23.9            ~ 2,
    score >= 17.8           ~ 3,
    score >= 12.85            ~ 4,
    !is.na(score)            ~ 5,  # Least deprived
    TRUE                     ~ NA_integer_
  )
}
df_2021_adults <- df_2021_adults %>%
  mutate(imd_quintile = assign_quintile_2021(imd_score))

