  ~id,     ~strk_y_manual,
  "9650",2022,"9670",2022,"9798",2022,"9807",2022,"9911",2023,"9948",2023,
  "9986",2022,"9987",2023,"9998",2023,"9999",2017,"10000",2019,"10008",2023,
